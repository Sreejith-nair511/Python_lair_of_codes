#factorial kurma curry


def factorial(num):
    if num==0:
        return 1
    else:
        return num*factorial(num-1)
    
n=int(input('enter the value of n:'))
r=int(input('enter the r value(r cannot be negative or greater than n)'))
nCr=factorial(n)/factorial(r)*factorial(n-r)
print(n,'C',r," = ","%d"%nCr,sep="")

    
